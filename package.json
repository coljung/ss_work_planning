{
  "name": "ui-xxx",
  "version": "0.1.10",
  "description": "Micro Service Name",
  "author": "SSENSE",
  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "dev": "NODE_ENV=development webpack-dev-server --color --hot --inline --config ./webpack.dev.config.js --content-base ./public",
    "build": "NODE_ENV=production UV_THREADPOOL_SIZE=100 webpack --color --config ./webpack.prod.config.js",
    "lint": "eslint 'app/**'",
    "lint:fix": "eslint 'app/**' --fix",
    "test": "jest",
    "test:unit": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "cover": "npm run test:unit",
    "coveralls": "cat tests/coverage/unit/lcov.info | coveralls",
    "precommit": "npm run lint",
    "prepush": "npm run test"
  },
  "dependencies": {
    "@handsontable/react": "^1.0.0",
    "@ssense/framework": "^0.8.0",
    "antd": "^3.7.0",
    "body-parser": "^1.18.3",
    "config": "^1.29.4",
    "cors": "^2.8.1",
    "express": "^4.16.2",
    "handsontable-pro": "^5.0.0",
    "history": "^3.2.0",
    "http-proxy-middleware": "^0.17.4",
    "i18next": "^11.3.6",
    "moment": "^2.22.2",
    "newrelic": "^4.1.4",
    "node-basicauth": "0.0.9",
    "offline-plugin": "^4.9.1",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-hot-loader": "^3.1.3",
    "react-redux": "^5.0.7",
    "react-router": "^3.2.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.3.0",
    "remote-redux-devtools": "^0.5.7",
    "request": "^2.87.0",
    "superagent": "^3.8.3",
    "superagent-promise": "^1.1.0"
  },
  "devDependencies": {
    "@alienfast/i18next-loader": "^1.0.16",
    "@storybook/addon-actions": "^3.4.8",
    "@storybook/addon-links": "^3.4.8",
    "@storybook/react": "^3.4.8",
    "autoprefixer": "^7.2.6",
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.2.1",
    "babel-jest": "^23.4.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-import": "^1.8.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.16.3",
    "babel-runtime": "^6.11.6",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "coveralls": "^3.0.2",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-ui-core": "^0.2.2",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.10.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "fetch-mock": "^6.5.0",
    "file-loader": "^0.11.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^2.24.1",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "istanbul": "^0.4.5",
    "istanbul-api": "1.2.2",
    "istanbul-reports": "1.1.4",
    "jest": "^23.4.1",
    "jest-webpack-alias": "^3.3.3",
    "jsdom": "^11.11.0",
    "less": "^2.7.3",
    "less-loader": "^4.1.0",
    "less-vars-to-js": "^1.2.1",
    "nock": "^9.4.1",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "postcss-loader": "^2.1.5",
    "progress-bar-webpack-plugin": "^1.11.0",
    "react-router-dom": "^4.3.1",
    "react-test-renderer": "^16.4.1",
    "redux-mock-store": "^1.5.3",
    "sinon": "^2.1.0",
    "style-loader": "^0.17.0",
    "url-loader": "^0.5.7",
    "webpack": "^3.12.0",
    "webpack-dashboard": "^1.1.1",
    "webpack-dev-middleware": "^1.12.2",
    "webpack-dev-server": "^2.11.2",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "jest-webpack-alias": {
    "configFile": "webpack.dev.config.js"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/tests/shim.js",
      "<rootDir>/tests/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/tests/unit/**/*.{js,jsx}"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageDirectory": "<rootDir>/tests/coverage/unit",
    "coverageReporters": [
      "lcov",
      "json",
      "html",
      "text",
      "text-summary"
    ],
    "transform": {
      "^.+\\.(css|less|scss)$": "identity-obj-proxy",
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/tests/helpers/styleMock.js",
      "notifications(.*)$": "<rootDir>/app/notifications/$1"
    },
    "bail": true,
    "verbose": true,
    "globals": {
      "__DEV__": true,
      "__HOT_BUILD_DATE__": "",
      "__HOT_PACKAGE_NAME__": "",
      "__HOT_VERSION__": "",
      "__HOT_BASE_VERSION__": "",
      "UI_PLANNING_HOST": "http://127.0.0.1"
    },
    "collectCoverageFrom": [
      "**/app/**/*.{js,jsx}",
      "!app/index.jsx",
      "!app/observers/observetime.js",
      "!**/app/styles/**",
      "!**/__mocks__/**",
      "!**/__tests__/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 5,
        "functions": 15,
        "lines": 15,
        "statements": 0
      }
    }
  }
}
