{
  "extends": "ui-core/client-react",
  "plugins": ["react"],
  "settings": {
    'import/resolver': ['webpack']
  },
  "rules": {
    "indent": [
      2,
      4,
      {
        "SwitchCase": 1,
        "MemberExpression": 1,
        "ObjectExpression": 1,
        "CallExpression": {
          "arguments": 1
        }
      }
    ]
  }
}
